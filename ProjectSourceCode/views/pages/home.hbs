
{{> head}}
<body>
    {{> nav}}
    {{> message}}
    {{!add css to make look pretty}}
    <h1>Welcome to BuffAI</h1>
    
    <div class="progress-container">
        <span class="progress-text">{{progress}}%</span>
    </div>
    
    <div class="section">Degree Plan</div>
    {{#each degreePlan}}
        <div class="section">{{this}}</div>
    {{/each}}
    
    <div class="section">Credit Hours Taken: {{creditHours}}</div>
    <div class="section">Weekly Schedule</div>
    
    {{> footer}}
</body>
</html>